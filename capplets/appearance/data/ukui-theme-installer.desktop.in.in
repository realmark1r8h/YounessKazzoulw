[Desktop Entry]
_Name=Theme Installer
_Comment=Installs themes packages for various parts of the desktop
Exec=ukui-appearance-properties -i %u
Icon=preferences-desktop-theme
Terminal=false
Type=Application
MimeType=application/x-ukui-theme-package;
StartupNotify=true
Categories=GTK;Settings;
Keywords=ukui-control-center;UKUI;install;desktop;themes;
NoDisplay=true
NotShowIn=KDE;GNOME;MATE;
